    <%= simple_form_for [ @pokemon, @booking ] do |f| %>
      <%= f.label :start_date %>
      <%= f.date_field :start_date, class: "form-control mb-2 mr-sm-2"%>
      <%= f.label :end_date %>
      <%= f.date_field :end_date, class: "form-control mb-2 mr-sm-2" %>
      <%= f.button :submit, class: "btn-pokemon mt-2 mb-2 mr-sm-2" %>
      <%= link_to "Back", bookings_path, class: "btn-pokemon py-2 px-3 mt-2 mb-2 mr-sm-2" %>
    <% end %>   
