<div class="img-map-card">
  <% if pokemon.photo.attached? %>
    <%= cl_image_tag pokemon.photo.key %>
  <% else %>
    <%= [image_tag("https://res.cloudinary.com/markcarson121/image/upload/v1621537842/snorlax_tayhnt.png"), image_tag("https://res.cloudinary.com/markcarson121/image/upload/v1621537841/bulbsaur_qz7jh4.png"), image_tag("https://res.cloudinary.com/markcarson121/image/upload/v1621537842/charizard_r9uub6.png"), image_tag("https://res.cloudinary.com/markcarson121/image/upload/v1621537840/pachi_zosvdq.png"), image_tag("https://res.cloudinary.com/markcarson121/image/upload/v1621537840/eevee_jmi0qq.png"), image_tag("https://res.cloudinary.com/markcarson121/image/upload/v1621538125/pikachu_qfugzj.png")].sample %>
  <% end %>  
  </div>
<h2><%= link_to pokemon.name, pokemon_path(pokemon), :class => "show-link" %></h2>
<p><%= pokemon.address %></p>